
<%= form_for @students do |f| %>
<div>
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
<div>
  <%= f.label :age %>
  <%= f.number_field :age %>
</div>
<div>
  <%= f.label :address %>
  <%= f.text_field :address %>
</div>
<div>
  <%= f.label :percentage %>
  <%= f.number_field :percentage %>
</div>
<div>
  <%= f.submit %>
</div>
<% end %>
<title>student app</title>
<h1 style='text-align:center'>STUDENT INFORMATION</h1>
<body style='margin:  30px auto;
    background-color:lightgrey;
    width: 50%;
    border: 2px solid black;
    padding: 10px 40px 30px;'>
<div style ='text-align:center'>


    <%= link_to 'New Student', new_student_detail_path %>
    <table class="table table-striped" style="text-align:center">
  <tr>
    <th>Sem No</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
 
<% @student_details.each do |student| %>
  <tr>
    <td><%= student.usn %></td>
    <td><%= student.name %></td>
    <td><%= link_to 'Show', student %></td>
    <td><%= link_to 'Edit', edit_student_detail_path(student) %></td>
    <td><%= link_to 'Destroy', student, :confirm => 'Are you sure?',
                                     :method => :delete %></td>
  </tr>
<% end %>
</table>
 
<br />
 
<%#= link_to 'New Student', new_student_detail_path %>